# CultureCart Frontend Environment Variables

# Appwrite Configuration
VITE_APPWRITE_ENDPOINT=https://nyc.cloud.appwrite.io/v1
VITE_APPWRITE_PROJECT_ID=68fffa88000672ed9794
VITE_APPWRITE_DATABASE_ID=culturecart_db

# Google Maps API (for location services)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Payment Gateway (Razorpay)
VITE_RAZORPAY_KEY_ID=your-razorpay-key-id

# Analytics (Mixpanel)
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# AI Services (Gemini)
VITE_GEMINI_API_KEY=your-gemini-api-key

# Cloudinary (for image processing)
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Email Service (for contact forms)
VITE_EMAILJS_SERVICE_ID=your-emailjs-service-id
VITE_EMAILJS_TEMPLATE_ID=your-emailjs-template-id
VITE_EMAILJS_PUBLIC_KEY=your-emailjs-public-key

# Social Media Links
VITE_INSTAGRAM_URL=https://instagram.com/culturecart
VITE_FACEBOOK_URL=https://facebook.com/culturecart
VITE_TWITTER_URL=https://twitter.com/culturecart

# Support Contact
VITE_SUPPORT_EMAIL=support@culturecart.com
VITE_SUPPORT_PHONE=+91-XXXXXXXXXX

# App Configuration
VITE_APP_NAME=CultureCart
VITE_APP_URL=https://culturecart.com
VITE_APP_VERSION=2.0.0

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_3D_MODELS=true
VITE_ENABLE_VOICE_SEARCH=true
VITE_ENABLE_REAL_TIME_NOTIFICATIONS=true

# Development
VITE_ENABLE_DEBUG_MODE=false
